generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Receipt {
  id                  String    @id @default(uuid())
  filename            String?
  status              String
  image               String?
  customer            String?
  supplier            String?
  receiptValueInCents Int?
  issValueInCents     Int?
  receiptNumber       String?
  documentType        String?
  issueDate           DateTime?
  accrualDate         DateTime?

  @@map("receipts")
}
